
public class greater
{
    public static void main(String[] args) 
    {
       int a,b,c;
       a=4;
       b=5;
       c=13;
       
       if(a>b && a>c)
          System.out.println("a ");
        else if(b>c && b>a)
          System.out.println("b");
        else
          System.out.println("c");
       
       
    }
}
